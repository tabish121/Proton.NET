<Project Sdk="Microsoft.NET.Sdk">

  <ItemGroup>
    <ProjectReference Include="..\Proton\Proton.csproj" />
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>Proton.Client.Tests,
      </_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <OutputPath>..\..\build\</OutputPath>
    <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>

  <PropertyGroup>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>Qpid.Proton.NET.Client</PackageId>
    <Version>1.0.0-M1</Version>
    <Authors>Apache Qpid</Authors>
    <Company>Apache Software Foundation</Company>
    <Product>Apache Qpid Proton .NET Imperative API Client</Product>
    <Description>Apache Qpid Proton .NET an AMQP client featuring an imperative API</Description>
    <Copyright>Copyright (C) 2020-2022 Apache Software Foundation</Copyright>
    <PackageProjectUrl>https://qpid.apache.org/proton/</PackageProjectUrl>
    <PackageLicense>https://github.com/tabish121/Proton.NET/blob/main/LICENSE.txt</PackageLicense>
    <PackageIconUrl>https://qpid.apache.org/assets/img/qpid_logo_icon.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/tabish121/Proton.NET</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>apache;qpid;proton;amqp;protocol;client;imperative</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../../LICENSE" Pack="true" PackagePath="LICENSE" />
    <None Include="../../NOTICE" Pack="true" PackagePath="NOTICE" />
  </ItemGroup>

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>../../key\proton-dotnet.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

</Project>
